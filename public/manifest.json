{
  "manifest_version": 3,
  "name": "Ex Freedium",
  "version": "1.0",
  "description": "this extension open mediun.com in Freedium",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon2.png"
  },
  "options_ui": {
    "page": "popup.html",
    "open_in_tab": false
  },
  "icons": {
    "128": "icon2.png",
    "48": "icon2.png",
    "16": "icon2.png",
    "32": "icon2.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab"
  ],
  "host_permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
